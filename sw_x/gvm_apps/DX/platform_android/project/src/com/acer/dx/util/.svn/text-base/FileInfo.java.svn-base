package com.acer.dx.util;

public class FileInfo {

    public enum Type {
        Folder,
        File
    }
    public enum Status {
        ServerOnly,
        Synced,
        Syncing,
        WaitForSyncing,
        Conflict
    }
    private String mFileId;
    private String mFileName;
    private Type mFileType;
    private String mDeviceId;
    private String mDeviceName;
//    private CategoryType mCategoryType;
    private String mCloudPath;
    private String mLocalPath;
    private long mFileSize;
    private String mModifiedTime;
    private String mFileUri;
    private String mThumbnailUri;
    private String mMime;
    private Status mFileStatus;
    
    public void setFileId(String id) {
        mFileId = id;
    }
    
    public String getFiledId() {
        return mFileId;
    }
    
    public void setFileName(String name) {
        mFileName = name;
    }
    
    public String getFileName() {
        return mFileName;
    }
    
    public void setFileType(Type type) {
        mFileType = type;
    }
    
    public Type getFileType() {
        return mFileType;
    }
    
    public void setDeviceId(String id) {
        mDeviceId = id;
    }
    
    public String getDeviceId() {
        return mDeviceId;
    }
    
    public void setDeviceName(String name) {
        mDeviceName = name;
    }
    
    public String getDeviceName() {
        return mDeviceName;
    }
/*    
    public void setCategoryType(CategoryType type) {
        mCategoryType = type;
    }
    
    public CategoryType getCategoryType() {
        return mCategoryType;
    }
*/    
    public void setCloudPath(String path) {
        mCloudPath = path;
    }
    
    public String getCloudPath() {
        return mCloudPath;
    }
    
    public void setLocalPath(String path) {
        mLocalPath = path;
    }
    
    public String getLocalPath() {
        return mLocalPath;
    }
    
    public void setFileSize(long size) {
        mFileSize = size;
    }
    
    public long getFileSize() {
        return mFileSize;
    }
    
    public void setModifyTime(String time) {
        mModifiedTime = time;
    }
    
    public String getModifyTime() {
        return mModifiedTime;
    }
    
    public void setFileUri(String uri) {
        mFileUri = uri;
    }
    
    public String getFileUri() {
        return mFileUri;
    }
    
    public void setThumbnailUri(String uri) {
        mThumbnailUri = uri;
    }
    
    public String getThumbnailUri() {
        return mThumbnailUri;
    }
    
    public void setMime(String mime) {
        mMime = mime;
    }
    
    public String getMime() {
        return mMime;
    }
    
    public void setFileStatus(Status status) {
        mFileStatus = status;
    }
    
    public Status getFileStatus() {
        return mFileStatus;
    }
}
